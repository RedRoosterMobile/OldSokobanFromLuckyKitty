/**
 * css for iphone 3GS and up -  320 x 480 px 
 * todo: http://www.cssdrive.com/compressor/compress.php
 * better http://devilo.us/#
 * can save 45 % aka 15KB
 */
		* {
			margin:0;border:0;padding:0;
			font-family: "Arial Rounded MT Bold", Arial, sans-serif;
			letter-spacing: 0.05em;
			line-height: 1.4em;
			font-size: 12px;
			color:white;
			-webkit-user-select: none;
			-webkit-touch-callout: none;
		}
		
		body {
		 background-color: #000;
		}
		
		a {
			text-decoration:none;
			-webkit-touch-callout: none;
		}
		a:visited {
			text-decoration:none;
		}
		
		img {
			border:0;
		}
		.hide {
			display:none;
		}
		.clear {
			clear:both;
		}
		.floatLeft {
			float:left;
		}
		.floatRight {
			float:right;
		}
		#content {
			width: 320px;
			height: 480px;
		}
		/* Different backgrounds */
		
		#content.redStar {
			background: url('../images/new/bg/RedStar.jpg') 0 0 no-repeat;
		}
		#content.redStar.dark {
