			top:80px;
		}
		
		@-webkit-keyframes cherryBlossom1 {
			from {-webkit-transform: translate3d(0px,0px,0) rotateZ(0deg);opacity:1;-webkit-animation-timing-function: ease-out;}
			5% {-webkit-transform: translate3d(10px,-32px,0) rotateZ(30deg) ;opacity:1;-webkit-animation-timing-function: ease-in;}
			20% {-webkit-transform: translate3d(20px,22px,0) rotateZ(120deg);opacity:1;-webkit-animation-timing-function: linear;}
			to {-webkit-transform: translate3d(60px,300px,0) rotateZ(720deg);opacity:0;}
		}
		@-webkit-keyframes cherryBlossom2 {
			from {-webkit-transform: translate3d(0px,0px,0) rotateZ(0deg);opacity:1;-webkit-animation-timing-function: ease-out;}
			5% {-webkit-transform: translate3d(-10px,-34px,0) rotateZ(-40deg);opacity:1;-webkit-animation-timing-function: ease-in;}
			20% {-webkit-transform: translate3d(-22px,26px,0) rotateZ(-140deg);opacity:1;-webkit-animation-timing-function: linear;}
			to {-webkit-transform: translate3d(-64px,300px,0) rotateZ(-700deg);opacity:0;}
		}
		@-webkit-keyframes cherryBlossom3 {
			from {-webkit-transform: translate3d(0px,0px,0) rotateZ(0deg);opacity:1;-webkit-animation-timing-function: ease-out;}
			5% {-webkit-transform: translate3d(10px,-32px,0) rotateZ(60deg) rotateX(30deg) ;opacity:1;-webkit-animation-timing-function: ease-in;}
			20% {-webkit-transform: translate3d(35px,22px,0) rotateZ(150deg);opacity:1;-webkit-animation-timing-function: linear;}
			to {-webkit-transform: translate3d(60px,300px,0) rotateZ(780deg);opacity:0;}
		}
		@-webkit-keyframes cherryBlossom4 {
			from {-webkit-transform: translate3d(0px,0px,0) rotateZ(0deg);opacity:1;-webkit-animation-timing-function: ease-out;}
			5% {-webkit-transform: translate3d(-10px,-34px,0) rotateZ(-40deg);opacity:1;-webkit-animation-timing-function: ease-in;}
			20% {-webkit-transform: translate3d(-22px,26px,0) rotateZ(-170deg);opacity:1;-webkit-animation-timing-function: linear;}
			to {-webkit-transform: translate3d(-64px,300px,0) rotateZ(-760deg);opacity:0;}
		}
		
