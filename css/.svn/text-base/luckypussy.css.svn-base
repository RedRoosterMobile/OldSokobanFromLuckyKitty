/* ============================================================================================================================
== BACKGROUNDS (more CSS3)
** ============================================================================================================================ */
		#backgrounds {
			padding-left:5px;
			padding-top:5px;
			
		}
		.background ,.settingsCat{
			width:100px;
			height:100px;
